<%include partials/header%>
<%include partials/sidebar%>
    <div class="col-md-9">
      <h4 id = 'woman'>Woman</h4>
      <hr>
      <div class="row">
        <% prodwoman.forEach(function(product){ %>
        <div class="col-xs-6 col-sm-4 col-lg-3 col-md-3">
          <div class="thumbnail">
            <img style = 'max-height: 140px'src=<%= product.productimg%> alt="">
            <div style = 'height: 165px' class="caption">
              <h4 class= 'title'>
                <%= product.productname%>
              </h4>
              <p style = 'color: #777;'><%= product.productsize%> ml</p>
              <p><strong>ID : </strong><%= product.productid%></p>
              <p style = 'color: #777; font-size: 20px'>₡ <%= product.productprice%></p>
              <p class="ratings"><%= product.quantity%> in stock</p>
            </div>
          </div>
        </div>
        <%})%>
      </div>
      <h4 id = 'man'>Man</h4>
      <hr>
      <div class="row">
        <% prodman.forEach(function(product){ %>
        <div class="col-xs-6 col-sm-4 col-lg-3 col-md-3">
          <div class="thumbnail">
            <img style = 'max-height: 140px'src=<%= product.productimg%> alt="">
            <div style = 'height: 165px' class="caption">
              <h4 class= 'title'>
                <%= product.productname%>
              </h4>
              <p style = 'color: #777;'><%= product.productsize%> ml</p>
              <p><strong>ID : </strong><%= product.productid%></p>
              <p style = 'color: #777;font-size: 20px'>₡ <%= product.productprice%></p>
              <p class="ratings"><%= product.quantity%> in stock</p>
            </div>
          </div>
        </div>
        <%})%>
      </div>
    </div>
  </div>
</div>
<%include partials/footer%>
